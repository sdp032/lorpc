{
  "configuration": {
    "status": "info",
    "name": "mars",
    "appenders": {
      "Console": {
        "name": "STDOUT",
        "PatternLayout": {
          "pattern": "%d %p %c{1.} [%t] $${env:USER} %m%n"
        }
      }
    },
    "loggers": {
      "root": {
        "level": "info",
        "AppenderRef": {
          "ref": "STDOUT"
        }
      }
    }
  }
}