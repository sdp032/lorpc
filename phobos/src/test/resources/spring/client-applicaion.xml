<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:phobos="http://jkys.com/mars/phobos"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://jkys.com/mars/phobos
       http://jkys.com/mars/phobos.xsd">

    <phobos:subscribe xbusAddr="localhost:8082,localhost:8083" addr="localhost:8080"/>

    <bean id="test" class="com.jkys.phobos.spring.client.beans.PhobosFactoryBean">
        <property name="phobosInterface" value="com.jkys.phobos.service.TestService"/>
    </bean>
</beans>